				;--------------------------------------------------------------
				; findAnswer
				; calculates either addition or subtracion of two 80-bit
				; numbers depending on the value of the control flag (bl register)
				; Receives: esi = memory offset of the first operand
				;		    edi = memory offset of the second operand
				;			edx = offset of the location where the result of the operation is to be stored
				;			bl = control flag to specify the desired operation (addition: 0, subtraction: non-zero)
				; Returns: eax = sum or diffrence of two 80-bit numbers
				;-------------------------------------------------------------

 000000B5			findAnswer proc
 000000B5  80 FB 00			 CMP bl,0						; compare the contents of the bl register to 0
 000000B8  74 02			 JE doAdd						; if bl is equal to 0 then jump to doAdd
 000000BA  75 1C			 JNE doSub						; if bl is not equal to 0 then jump to doSub

					 ; add the two register values
 000000BC				 doAdd:							
 000000BC  8B 06			 mov eax, [esi]					; move first numbers first 32-bits to eax		
 000000BE  03 07			 add eax, [edi]					; add the first numbers first 32-bits to the second numbers first 32-bits	
 000000C0  89 02			 mov [edx],eax					; move the result of the addition of the first 32-bits to edx register
					 
 000000C2  8B 46 04			 mov eax, [esi+4]				; move first numbers next 32-bits to eax	
 000000C5  13 47 04			 adc eax, [edi+4]				; add first numbers next 32-bits to the second numbers next 32-bits	
 000000C8  89 42 04			 mov [edx+4], eax				; move the result of the addition of the 32-bits to edx register

 000000CB  66| 8B 46 08			 mov ax, [esi+8]				; move first numbers next 16-bits to eax
 000000CF  66| 13 47 08			 adc ax, [edi+8]				; add first numbers next 16-bits to the second numbers next 16-bits
 000000D3  66| 89 42 08			 mov [edx+8],ax					; move the result of the addition of next 16-bits to edx register
 000000D7  C3				 ret							; return to main procedure

